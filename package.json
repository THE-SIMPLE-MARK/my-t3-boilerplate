{
	"name": "my-t3-boilerplate",
	"version": "0.1.0",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "NODE_OPTIONS=\"--no-deprecation\" next dev --turbopack --experimental-https --port 3000",
		"start": "next start",
		"start:dev": "NODE_OPTIONS=\"--no-deprecation\" SKIP_ENV_VALIDATION=true VERCEL_URL=localhost:3000 next start",
		"lint": "biome lint",
		"lint:fix": "biome lint --write",
		"format": "biome format --write",
		"db:gen": "prisma generate --no-hints",
		"db:gen:watch": "prisma generate --no-hints --watch",
		"db:push": "prisma db push",
		"db:reset": "prisma db push --force-reset",
		"db:dev": "prisma studio --browser none",
		"db:dev:init": "cd scripts && docker compose up -d",
		"postinstall": "prisma generate --no-hints"
	},
	"dependencies": {
		"@prisma/adapter-pg": "^7.0.1",
		"@prisma/client": "^7.0.1",
		"@prisma/client-runtime-utils": "^7.0.1",
		"@t3-oss/env-nextjs": "^0.13.8",
		"@tanstack/react-query": "^5.90.11",
		"@trpc/client": "^11.7.2",
		"@trpc/react-query": "^11.7.2",
		"@trpc/server": "^11.7.2",
		"@trpc/tanstack-react-query": "^11.7.2",
		"dotenv": "^17.2.3",
		"next": "^16.0.4",
		"react": "^19.2.0",
		"react-dom": "^19.2.0",
		"server-only": "^0.0.1",
		"superjson": "^2.2.5",
		"zod": "^4.1.13"
	},
	"devDependencies": {
		"@eslint/eslintrc": "^3.3.1",
		"@eslint/js": "^9.39.1",
		"@next/eslint-plugin-next": "^16.0.4",
		"@tailwindcss/postcss": "^4.1.17",
		"@types/node": "^24.10.1",
		"@types/react": "^19.2.7",
		"@types/react-dom": "^19.2.3",
		"eslint": "^9.39.1",
		"eslint-config-next": "^16.0.4",
		"eslint-plugin-react-hooks": "^7.0.1",
		"lefthook": "^2.0.4",
		"postcss": "^8.5.6",
		"prettier": "^3.6.2",
		"prettier-plugin-tailwindcss": "^0.7.1",
		"prisma": "^7.0.1",
		"tailwindcss": "^4.1.17",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.48.0"
	},
	"ct3aMetadata": {
		"initVersion": "7.39.3"
	},
	"packageManager": "pnpm@10.15.1"
}
